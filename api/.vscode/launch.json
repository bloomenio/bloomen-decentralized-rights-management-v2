    "version": "0.2.0",
    "configurations": [
        
        {
        "type": "node",
        "request": "launch",
        "name": "API (npm start:dev)",
        "cwd": "${workspaceFolder}/api/",
        // "envFile": ".env",
        "runtimeExecutable": "npm",
        "runtimeArgs": [
            "run-script",
            "start:dev"
        ],
        "protocol": "auto",
        "restart": true,
        "sourceMaps": true,
        "console": "integratedTerminal",
        "internalConsoleOptions": "openOnSessionStart",
        "port": 9229
    }]
}